#alias {_zabij_} 
{
	#parse {$pwskazywanie} {wskazanie_typ} {
      #switch {"$wskazanie_typ"} {
        #case {"d"} khpiesn;                      #nop druchii;
        #case {"u"} drwskaz %0;                   #nop upiorny pancerz;
        #case {"z"} zabijcmd %0;                  #nop komenda zabij;
        #case {"w"} wskaz %0 jako cel ataku;      #nop wskazywanie;
        #case {"r"} rozkaz druzynie zabic %0;     #nop rozkaz;
        #nop case {"p"} przelam obrone celu ataku;#nop przelamanie TODO;
      };
    };
}

#alias {_zaznaczcel_} {
  zabijcmd 1. wroga;
  zabijcmd 2. wroga;
  zabijcmd 3. wroga;
  zabijcmd 4. wroga;
  wesprzyj;
  #parse {$pwskazywanie} {wskazanie_typ} {
      #switch {"$wskazanie_typ"} {
        #case {"d"} khpiesn;                             #nop druchii;
        #case {"u"} drwskaz cel ataku;                   #nop upiorny pancerz;
        #case {"z"} zabijcmd cel ataku;                  #nop komenda zabij;
        #case {"w"} wskaz cel ataku jako cel ataku;      #nop wskazywanie;
        #case {"r"} rozkaz druzynie zabic cel ataku;     #nop rozkaz;
        #nop case {"p"} przelam obrone celu ataku;#nop przelamanie TODO;
      };
    };
}

#action {sie %1 {sztuk|sztuki}.} {
  #var liczba_zabitych 0;
  #switch {"%1"} {
    #case {"jednej"} {#var liczba_zabitych {1}};
    #case {"dwoch"} {#var liczba_zabitych {2}};
    #case {"trzech"} {#var liczba_zabitych {3}};
    #case {"czterech"} {#var liczba_zabitych {4}};
    #case {"pieciu"} {#var liczba_zabitych {5}};
    #case {"szesciu"} {#var liczba_zabitych {6}};
    #case {"siedmiu"} {#var liczba_zabitych {7}};
    #case {"osmiu"} {#var liczba_zabitych {8}};
    #case {"dziewieciu"} {#var liczba_zabitych {9}};
    #case {"dziesieciu"} {#var liczba_zabitych {10}};
    #case {"jedenastu"} {#var liczba_zabitych {11}};    
  };
  #if {"$zbieranie_ready" == "true"} {
    qb _zbierzloop_;
  };
}

#alias {_zbierz_} {
  policz ciala;
  #var zbieranie_ready {true};
}

#alias {_zbierzloop_}
{
  #loop {1} {$liczba_zabitych} {liz} {
    #parse {$pzbieranie} {zbierany_typ} {
      #switch {"$zbierany_typ"} {
        #case {"k"} {wez kamienie z $liz\. ciala;};
        #case {"m"} {wez monety z $liz\. ciala;};
        #case {"i"} {wez przedmioty z $liz\. ciala;};
        #case {"d"} {khdorznij $liz\. cialo;obe fiolke};
        #case {"h"} {hwodetnij ucho od $liz\. ciala;};
      };
    };
  };
  #var zbieranie_ready {false};
};

