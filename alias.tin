#alias {qr} {
  #line substitute {variables} {#macro {$KEY_QB} {%0}};
  #line substitute {variables} {#macro {$KEY_PAD_MIDDLE} {%0}};

  #format {show_bind_} {%h} { %0 };
  #echo {${c_stan_sredni}$show_bind_};
}

#alias {qk} {
  #var r @_print_war_bind{%0};
