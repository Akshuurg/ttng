#event {PROGRAM START} {
	#var cport %0;
	#line substitute {variables} {#chat init {${cport}1}};
	#map read bundles/TTMap/map.map;
	#map flag vtmap on;
	#map flag unicodegraphics on;
	#delay {3} {
		#format sw {%R};
		#split $sw 0;
	};
};

#action {chats to you, '%d'} {
	#map goto %1;
	#line gag;
};
