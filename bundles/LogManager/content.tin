#alias {logowanie} {
	#if {"$ConfigManager_Configs[LogManagerLogging]" == "wlaczone"} {
		#config LOG plain;
		#format {logstamp} {%t} {%Y-%m-%d};
		#line sub variables {#log append {bundles/ConfigManager/Profiles/Logs/$AVATAR_NAME-$logstamp}};
	};	
};

#alias {echokomend} {
	#if {"$ConfigManager_Configs[TTConfigCommandEcho]" == "wlaczone"} {
		#config {COMMAND ECHO} {on};
	} {
		#config {COMMAND ECHO} {off};
	};
}
