#alias {/mark as %w$} {
    #if {$PONGED == 1} {

    #switch {"%1"} {
        #case {"vendor"} {};
        #case {"checkpoint"} {};
        #case {"water"} {};
        #case {"bank"} {};
        #case {"post"} {};
        #case {"safe"} {};
        #case {"food"} {};
        #case {"quest"} {};
        #case {"info"} {};
        #case {"danger"} {};
 	#case {"port"} {};
	#case {"herb"} {};
	#case {"trap"} {};
	#case {"smith"} {};
 	#case {"training"} {};
	#case {"knowledge"} {};
	#case {"storage"} {};
        #default {
            #echo {Niedozwolona wartosc: %1};
            #return;
        };
    }
    #map get roomvnum v;
    #chat message brain {MARK $v %1 $temp_short};
    } {show_ponged_error};
} {1};

