#action {<CHAT> File transfer from brain, file: %d, size: %d.} {
    #chat message mapka {DOWNLOAD notke %1};
    #chat accept brain;
} {1};

#action {<CHAT> Transfer of %d completed} {
    #line substitute {variables} {
        #system {mv %1 $path/notes/%1};
    };
} {1};

#alias {/event %*} {
