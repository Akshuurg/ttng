#action {Zapalasz} {
	#ticker lampa {
		qm lampa_napelnij;
	} {60};
};

#action {lampa wypala sie i gasnie} {
	qb lampa_napelnij;
};

#alias {lampa_napelnij} {
	wy oleje
	odloz pusta butelke;
	napelnij lampe olejem;
	zapal lampe;
	wl oleje;
};

#action {Gasisz olejna lampe} {
	#untick lampa;
};

#action {wypala sie i gasnie} {
	#untick lampa;
};
