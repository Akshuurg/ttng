#alias {lampa_napelnij} {
	wy oleje
	odloz pusta butelke;
	napelnij lampe olejem;
	zapal lampe;
	wl oleje;
};

#action {Zapalasz} {
	#ticker lampa {
		qm lampa_napelnij;
	} {60};
};

#action {lampa wypala sie i gasnie} {
	qr lampa_napelnij;
};

#action {Gasisz olejna lampe} {
	#untick lampa;
};

#action {wypala sie i gasnie} {
	#untick lampa;
	qr lampa_napelnij;
};

#action {Dopelniasz %*, oprozniajac zupelnie butelke oleju.} {
	Highlights_event %0;
	#line gag;
};
